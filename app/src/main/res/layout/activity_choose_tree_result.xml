<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        tools:context=".activity.ChooseTreeResultActivity">

        <ImageView
            android:id="@+id/logoImage"
            android:layout_width="@dimen/_110sdp"
            android:layout_height="@dimen/_60sdp"
            android:layout_marginTop="-5dp"
            android:background="@drawable/logo_one"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </ImageView>

        <ImageView
            android:id="@+id/backArrow"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/back"
            android:backgroundTint="@color/Black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </ImageView>

        <ImageView
            android:id="@+id/fruitTreeImage"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/backArrow"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/imageview_bg_3"
            android:layout_width="@dimen/_282sdp"
            android:layout_height="@dimen/_165sdp">

        </ImageView>

        <TextView
            android:id="@+id/fruitTreeText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Fruit Tree"
            android:layout_marginTop="@dimen/_7sdp"
            android:textSize="@dimen/_16ssp"
            android:fontFamily="@font/ubuntu_bold"
            android:textColor="@color/Black"
            app:layout_constraintStart_toStartOf="@+id/fruitTreeImage"
            app:layout_constraintTop_toBottomOf="@+id/fruitTreeImage" />

        <TextView
            android:id="@+id/fruitTreeResult"
            android:layout_width="@dimen/_282sdp"
            android:layout_height="wrap_content"
            android:text="Results hnkfdjvnbksdbvkbdskbvkcfbxkvbkxcnbvcxkvbkcxbnvknbcxknbvkcxnbkvn"
            android:layout_marginTop="@dimen/_7sdp"
            android:textSize="@dimen/_14ssp"
            android:fontFamily="@font/ubuntu_bold"
            android:textColor="@color/Black"
            app:layout_constraintTop_toBottomOf="@id/fruitTreeText"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <ImageView
            android:id="@+id/flowerTreeImage"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/fruitTreeResult"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/imageview_bg_3"
            android:layout_width="@dimen/_282sdp"
            android:layout_height="@dimen/_165sdp">

        </ImageView>

        <TextView
            android:id="@+id/flowerTreeText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_7sdp"
            android:fontFamily="@font/ubuntu_bold"
            android:text="Flower Tree"
            android:textColor="@color/Black"
            android:textSize="@dimen/_16ssp"
            app:layout_constraintStart_toStartOf="@+id/flowerTreeImage"
            app:layout_constraintTop_toBottomOf="@+id/flowerTreeImage" />

        <TextView
            android:id="@+id/flowerTreeResult"
            android:layout_width="@dimen/_282sdp"
            android:layout_height="wrap_content"
            android:text="Results hnkfdjvnbksdbvkbdskbvkcfbxkvbkxcnbvcxkvbkcxbnvknbcxknbvkcxnbkvn"
            android:layout_marginTop="@dimen/_7sdp"
            android:textSize="@dimen/_14ssp"
            android:fontFamily="@font/ubuntu_bold"
            android:textColor="@color/Black"
            app:layout_constraintTop_toBottomOf="@id/flowerTreeText"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <ImageView
            android:id="@+id/medicineTreeImage"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/flowerTreeResult"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/imageview_bg_3"
            android:layout_width="@dimen/_282sdp"
            android:layout_height="@dimen/_165sdp">

        </ImageView>

        <TextView
            android:id="@+id/medicineTreeText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_7sdp"
            android:fontFamily="@font/ubuntu_bold"
            android:text="Medicine Tree"
            android:textColor="@color/Black"
            android:textSize="@dimen/_16ssp"
            app:layout_constraintStart_toStartOf="@+id/medicineTreeImage"
            app:layout_constraintTop_toBottomOf="@+id/medicineTreeImage" />

        <TextView
            android:id="@+id/medicineTreeResult"
            android:layout_width="@dimen/_282sdp"
            android:layout_height="wrap_content"
            android:text="Results hnkfdjvnbksdbvkbdskbvkcfbxkvbkxcnbvcxkvbkcxbnvknbcxknbvkcxnbkvn"
            android:layout_marginTop="@dimen/_7sdp"
            android:textSize="@dimen/_14ssp"
            android:fontFamily="@font/ubuntu_bold"
            android:textColor="@color/Black"
            app:layout_constraintTop_toBottomOf="@id/medicineTreeText"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <ImageView
            android:id="@+id/economicalTreeImage"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/medicineTreeResult"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/imageview_bg_3"
            android:layout_width="@dimen/_282sdp"
            android:layout_height="@dimen/_160sdp">

        </ImageView>

        <TextView
            android:id="@+id/economicalTreeText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_7sdp"
            android:fontFamily="@font/ubuntu_bold"
            android:text="Economical Tree"
            android:textColor="@color/Black"
            android:textSize="@dimen/_16ssp"
            app:layout_constraintStart_toStartOf="@+id/economicalTreeImage"
            app:layout_constraintTop_toBottomOf="@+id/economicalTreeImage" />

        <TextView
            android:id="@+id/economicalTreeResult"
            android:layout_width="@dimen/_282sdp"
            android:layout_height="wrap_content"
            android:text="Results hnkfdjvnbksdbvkbdskbvkcfbxkvbkxcnbvcxkvbkcxbnvknbcxknbvkcxnbkvn"
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            android:textSize="@dimen/_14ssp"
            android:fontFamily="@font/ubuntu_bold"
            android:textColor="@color/Black"
            app:layout_constraintTop_toBottomOf="@id/economicalTreeText"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
